      "source": [
        "# Faça um programa que calcule o fatorial de um número inteiro fornecido pelo\n",
        "# usuário. Ex.: 5!=5.4.3.2.1=120\n",
        "numero=int(input(\"digite o valor que deseja fatorar: \"))\n",
        "fator = 1\n",
        "cont = 1\n",
        "\n",
        "while cont <= numero:\n",
        "  fator = fator * cont\n",
        "  cont += 1\n",
        "print(fator)\n"
      ]
    }
  ]
}